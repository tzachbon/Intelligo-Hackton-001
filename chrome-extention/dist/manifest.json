{
  "name": "Use Time",
  "author": "Tzach Bonfil",
  "version": "1.0.0",
  "icons": {
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "cookies",
    "storage",
    "tabs",
    "webRequest",
    "identity",
    "webRequestBlocking",
    "*://*/*"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/popup.js",
      "https://www.googletagmanager.com/gtag/js?l=dataLayer"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Kidsafe",
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [],
  "content_scripts": [
    {
      "js": ["js/eventPage.js", "js/webWorker.js"],
      "matches": ["*://*/*"]
    }
  ]
}
